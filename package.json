{
  "name": "WhoCallsTheFleet",
  "main": "app/main.html",
  "version": "0.24.0",
  "description": "An offline guidebook for KanColle. Based on NW.JS",
  "repository": {
    "type": "git",
    "url": "https://github.com/Diablohu/WhoCallsTheFleet"
  },
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/Diablohu/WhoCallsTheFleet"
    }
  ],
  "author": {
    "name": "Diablohu",
    "email": "diablohudream@gmail.com"
  },
  "readmeFilename": "README.md",
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://raw.github.com/cthackers/adm-zip/master/MIT-LICENSE.txt"
    }
  ],
  "debug": true,
  "nw-packager": {
    "version": "0.26.0",
    "platforms": [
      "win32",
      "win64",
      "osx64"
    ],
    "filesRelative": [
      "node_modules",
      "package.json"
    ],
    "dataVersion": {
      "app": "0.24.0.0",
      "app-db": "2017.10.26",
      "bgimgs": "1.18.6.0",
      "pics": "2017.10.26",
      "pics-enemies": "2017.08.12",
      "pics-ships": "2017.10.26",
      "pics-ships-extra": "2017.10.26"
    },
    "buildDir": "N:\\nwbuild\\build",
    "winIco": "./source/appicon.ico",
    "enableLauncher": true,
    "dataRelative": [
      "app",
      "app-db",
      "bgimgs",
      "pics",
      "pics-enemies",
      "pics-ships",
      "pics-ships-extra"
    ],
    "launcherSplash": "./source/launcher.webp",
    "macIcns": "./source/appicon.icns"
  },
  "window": {
    "width": 1280,
    "height": 720,
    "toolbar": false,
    "icon": "./app/assets/images/appicon.png",
    "position": "center",
    "min_width": 640,
    "min_height": 480,
    "as_desktop": false,
    "resizable": true,
    "always-on-top": false,
    "fullscreen": false,
    "show_in_taskbar": true,
    "frame": false,
    "show": false,
    "kiosk": false,
    "transparent": false
  },
  "platformOverrides": {
    "win": {
      "debug": false,
      "window": {
        "frame": false
      }
    },
    "win32": {
      "debug": false,
      "window": {
        "frame": false
      }
    },
    "osx": {
      "debug": false,
      "window": {
        "frame": true
      }
    },
    "linux": {
      "debug": false,
      "window": {
        "frame": true
      }
    }
  },
  "dependencies": {
    "adm-zip": "0.4.7",
    "jsonfile": "2.2.1",
    "markdown": "0.5.0",
    "mkdirp": "0.5.1",
    "nedb": "1.1.1",
    "q": "1.4.1",
    "request": "2.58.0",
    "request-progress": "0.3.1",
    "semver": "4.3.6"
  }
}
